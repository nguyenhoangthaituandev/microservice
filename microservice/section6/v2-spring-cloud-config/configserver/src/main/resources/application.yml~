spring:
  application:
    name: "configserver"
  profiles:
    #active: native
    active: git
  cloud:
    config:
      server:
        #native:
          #search-locations: "classpath:/config"
          #search-locations: "file:D:/config"
        git:
          uri: "git@github.com:nguyenhoangthaituandev/microservice.git"
          default-label: main
          timeout: 10000
          clone-on-start: true
          force-pull: true

server:
  port: 8071
